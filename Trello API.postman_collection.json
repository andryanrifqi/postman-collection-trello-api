	"item": [
		{
			"name": "Create a Board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const response = pm.response.json() ;\r",
							"\r",
							"pm.test(\"Board is created successfully\", () => {\r",
							"    pm.expect(response.name).to.be.eql(pm.collectionVariables.get(\"boardName\"));\r",
							"    pm.expect(response.closed).to.be.false;\r",
							"});\r",
							"\r",
							"pm.test(\"Board is private\", () => {\r",
							"    pm.expect(response.prefs.permissionLevel).to.be.eql(\"private\");\r",
							"})\r",
							"\r",
							"pm.test(\"Calendar is disabled\", () => { \r",
							"    const calendarView = response.prefs.switcherViews.find(view => view.viewType === \"Calendar\"); \r",
							"    pm.expect(calendarView).to.be.an(\"object\")\r",
							"    pm.expect(calendarView.enabled).to.be.false;\r",
							"})\r",
							"\r",
							"pm.collectionVariables.set(\"boardId\", response.id);\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let boardNumber = pm.collectionVariables.get(\"boardNumber\");\r",
							"\r",
							"if(isNaN(boardNumber)){\r",
							"    boardNumber = 1;\r",
							"    pm.collectionVariables.set(\"boardNumber\", boardNumber);\r",
							"    pm.collectionVariables.set(\"boardName\", \"Postman Exercise \" + boardNumber);\r",
							"}else if(boardNumber == 10){\r",
							"    pm.collectionVariables.unset(\"boardNumber\")\r",
							"    pm.collectionVariables.unset(\"boardName\")\r",
							"}else{\r",
							"    boardNumber++;\r",
							"    pm.collectionVariables.set(\"boardName\", \"Postman Exercise \" + boardNumber);\r",
							"    pm.collectionVariables.set(\"boardNumber\", boardNumber);\r",
							"}\r",
							"\r",
							"\r",
							"    \r",
							"    \r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
